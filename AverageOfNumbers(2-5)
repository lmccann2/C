#include <iostream>
using namespace std;



int main() {
  //variables
  double num1 = 28;
  double num2 = 32;
  double num3 = 37;
  double num4 = 24;
  double num5 = 33;
  double avg = (num1 + num2 + num3 + num4 + num5) / 5;
//Prints the average of the 5 numbers on display
  cout << "The Average of the five numbers is: " << avg << endl; 

//  std::cout << "Hello World!\n";
}
